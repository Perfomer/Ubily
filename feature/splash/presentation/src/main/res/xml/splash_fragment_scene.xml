<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1500">
        <KeyFrameSet>
            <KeyAttribute
                android:alpha="0"
                motion:framePosition="0"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:scaleX="100"
                motion:framePosition="0"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:scaleY="100"
                motion:framePosition="0"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:alpha="1"
                motion:framePosition="10"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:scaleX="1"
                motion:framePosition="40"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:scaleY="1"
                motion:framePosition="40"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:scaleX="0.75"
                motion:framePosition="50"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:scaleY="0.75"
                motion:framePosition="50"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:scaleX="1"
                motion:framePosition="60"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:scaleY="1"
                motion:framePosition="60"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:scaleX="1.075"
                motion:framePosition="72"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:scaleY="1.075"
                motion:framePosition="72"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:scaleX="1"
                motion:framePosition="80"
                motion:motionTarget="@+id/splash_logo_text" />

            <KeyAttribute
                android:scaleY="1"
                motion:framePosition="80"
                motion:motionTarget="@+id/splash_logo_text" />

        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start"></ConstraintSet>

    <ConstraintSet android:id="@+id/end"></ConstraintSet>
</MotionScene>